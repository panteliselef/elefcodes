{
	"version": 3,
	"routes": [
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^\\/_image$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/guestbook\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/guestbook\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/sign-in(?:\\/(.*?))?\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/sign-up(?:\\/(.*?))?\\/?$",
			"dest": "_render"
		}
	]
}